<root>

	<windowclass name="reference_armor" copy="record_window" />
	<windowclass name="reference_armor_header" copy="record_header" />
	<windowclass name="reference_armor_main">
		<margins control="0,0,0,7" />
		<sheetdata>
			<anchor_column name="columnanchor" />
			
			<label_column name="cost_label">
				<static textres="ref_label_list_cost" />
			</label_column>
			<string_column_left name="cost">
				<readonly />
			</string_column_left>
			
			<label_column name="armorpoints_label">
				<static textres="item_label_armorpoints" />
			</label_column>
			<number_columnh name="armorpoints" />

			<label_column name="damagereduction_label">
				<static textres="item_label_damagereduction" />
			</label_column>
			<number_columnh name="damagereduction" />

			<label_column name="o2supply_label">
				<static textres="item_label_o2supply" />
			</label_column>
			<string_column name="o2supply" />

			<label_column name="speed_label">
				<static textres="item_label_speed" />
			</label_column>
			<string_column name="speed" />

			<label_column name="special_label">
				<static textres="item_label_special" />
			</label_column>
			<string_column name="special">
				<readonly />
			</string_column>
						
			<line_column name="line_desc" />
			
			<string_columnh name="description">
				<anchored>
					<left offset="5" />
				</anchored>
				<separator>line_desc</separator>
				<readonly />
			</string_columnh>
		</sheetdata>
	</windowclass>

