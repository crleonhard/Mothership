			<label name="armorpoints_label">
				<anchored to="inventoryframe" position="insidebottomleft" offset="10,10" />
				<static textres="ARMOR" />
			</label>
			<number_stat name="armorpoints" source="armorpoints">
				<anchored to="inventoryframe" position="insidebottomleft" offset="110,10" width="60" height="20" />
				<readonly />
			</number_stat>

			<!-- <label name="damagereduction_label"> -->
