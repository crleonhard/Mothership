					<left offset="0" />
				</anchored>
				<target>credits.cr</target>
			</number_charcredits>
			<label_abilityscore>
				<anchored to="credits" />
				<static textres="CR" />
			</label_abilityscore>
			<number_charcredits name="kcr" source="credits.kcr">
				<target>credits.kcr</target>
			</number_charcredits>
			<label_abilityscore>
				<anchored to="kcr" />
				<static textres="KCR" />
			</label_abilityscore>
			<number_charcredits name="mcr" source="credits.mcr">
				<target>credits.mcr</target>
			</number_charcredits>
			<label_abilityscore>
				<anchored to="mcr" />
				<static textres="MCR" />
			</label_abilityscore>
			<number_charcredits name="bcr" source="credits.bcr">
				<target>credits.bcr</target>
			</number_charcredits>
			<label_abilityscore>
				<anchored to="bcr" />
				<static textres="BCR" />
			</label_abilityscore>

			<frame_char name="coinframe" merge="delete" />
